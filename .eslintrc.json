{
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-extra-boolean-cast": 0,
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}
